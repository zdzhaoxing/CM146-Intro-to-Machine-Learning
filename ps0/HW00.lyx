\begin_layout Enumerate
Matrix Decompositions
\end_layout

\begin_deeper
\begin_layout Enumerate
Given a square matrix
\begin_inset Formula $A\in\mathbb{R}^{n×n}$
\end_inset

, 
\begin_inset Formula $λ\in C$
\end_inset

 is an eigenvalue of 
\begin_inset Formula $A$
\end_inset

and
\begin_inset Formula $\mathbf{x}\in\mathbb{C}^{n}$
\end_inset

 is the corresponding eigenvector if 
\begin_inset Formula $A\mathbf{x}=\lambda\mathbf{x}$
\end_inset

,
\begin_inset Formula $\mathbf{x\neq0}$
\end_inset


\end_layout

\begin_layout Enumerate
Let det
\begin_inset Formula $\left(\begin{array}{cc}
\lambda-2 & -1\\
-1 & \lambda-2
\end{array}\right)=0$
\end_inset

, we get
\begin_inset Formula $(\lambda-2)^{2}-1=0$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\wasytherefore\lambda=1$
\end_inset

 or 3
\begin_inset Newline newline
\end_inset

When 
\begin_inset Formula $\lambda=1$
\end_inset

, 
\begin_inset Formula $\mathbf{x=\left(\begin{array}{c}
1\\
-1
\end{array}\right)}$
\end_inset


\begin_inset Newline newline
\end_inset

When 
\begin_inset Formula $\lambda=3$
\end_inset

, 
\begin_inset Formula $\mathbf{x=\left(\begin{array}{c}
1\\
1
\end{array}\right)}$
\end_inset


\end_layout

\begin_layout Enumerate
Proof by induction
\begin_inset Newline newline
\end_inset

Let 
\begin_inset Formula $\lambda_{i}$
\end_inset

and 
\begin_inset Formula $\mathbf{x}_{i}$
\end_inset

,
\begin_inset Formula $i=1,2,...,n$
\end_inset

, be eigenvalues and corresponding eigenvectors of A
\begin_inset Newline newline
\end_inset

When k = 1, 
\begin_inset Formula $A\mathbf{x_{\mathit{i}}}=\lambda_{i}^{1}\mathbf{x_{\mathit{i}}}$
\end_inset

by definition, so in this case the conclusion is true.
\begin_inset Newline newline
\end_inset

Suppose when k = N, 
\begin_inset Formula $\lambda_{1}^{N}$
\end_inset

,
\begin_inset Formula $\lambda_{2}^{N}$
\end_inset

,...,
\begin_inset Formula $\lambda_{n}^{N}$
\end_inset

are the eigenvalues of matrix 
\begin_inset Formula $A^{N}$
\end_inset

, 
\begin_inset Formula $\mathbf{x}_{1}$
\end_inset

,
\begin_inset Formula $\mathbf{x}_{2}$
\end_inset

,...,
\begin_inset Formula $\mathbf{x}_{n}$
\end_inset

are corresponding eigenvectors.
\begin_inset Newline newline
\end_inset

When k = N+1, 
\begin_inset Formula $A^{N+1}\mathbf{x}_{i}=A\left(A^{N}\mathbf{x}_{i}\right)=A\left(\lambda_{i}^{N}\mathbf{x}_{i}\right)=\lambda_{i}^{N}\mathbf{\mathit{A}x}_{i}=\lambda_{i}^{N+1}\mathbf{x}_{i}$
\end_inset

, the conclusion is also true for k =N+1.
\end_layout

\end_deeper
\begin_layout Enumerate
Vector and Matrix Calculus
\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $\mathbf{x}=\left(\begin{array}{c}
x_{1}\\
x_{2}\\
...\\
x_{n}
\end{array}\right)\in\mathbb{R}^{n}$
\end_inset

, 
\begin_inset Formula $\mathbf{a=\left(\mathrm{\mathit{\begin{array}{c}
a^{1}\\
a^{2}\\
...\\
a^{n}
\end{array}}}\right)}\in\mathbb{R}^{n}$
\end_inset

,
\begin_inset Formula $f(\mathbf{x})=\mathbf{a}^{T}\mathbf{x}=\sum a_{i}x_{i}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\nabla_{\mathbf{x}}f\left(\mathbf{x}\right)=\left(\begin{array}{c}
\frac{\partial f(\mathbf{x})}{\partial x_{1}}\\
\frac{\partial f(\mathbf{x})}{\partial x_{2}}\\
...\\
\frac{\partial f(\mathbf{x})}{\partial x_{n}}
\end{array}\right)=\left(\begin{array}{c}
a_{1}\\
a_{2}\\
...\\
a_{n}
\end{array}\right)=\mathbf{a}$
\end_inset


\end_layout

\begin_layout Enumerate
Let 
\begin_inset Formula $g(\mathbf{x})=\mathbf{x}^{T}A\boldsymbol{x}$
\end_inset

=
\begin_inset Formula $\sum_{i}\sum_{j}A_{ij}x_{i}x_{j}$
\end_inset

, A is symmetric, so 
\begin_inset Formula $A_{ij}=A_{ji}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\nabla_{\mathbf{x}}g\left(\mathbf{x}\right)=\left(\begin{array}{c}
\frac{\partial g(\mathbf{x})}{\partial x_{1}}\\
\frac{\partial g(\mathbf{x})}{\partial x_{2}}\\
...\\
\frac{\partial g(\mathbf{x})}{\partial x_{n}}
\end{array}\right)=$
\end_inset


\begin_inset Formula $\left(\begin{array}{c}
\sum A_{1i}x_{i}+\sum A_{i1}x_{i}\\
\sum A_{2i}x_{i}+\sum A_{i2}x_{i}\\
...\\
\sum A_{ni}x_{i}+\sum A_{in}x_{i}
\end{array}\right)=\left(\begin{array}{c}
2\sum A_{1i}x_{i}\\
2\sum A_{2i}x_{i}\\
...\\
2\sum A_{ni}x_{i}
\end{array}\right)=2A\mathbf{x}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\nabla_{\mathbf{x}}^{2}g\left(\mathbf{x}\right)=\left(\begin{array}{cccc}
\frac{\partial^{2}g(\mathbf{x})}{\partial x_{1}\partial x_{1}} & \frac{\partial^{2}g(\mathbf{x})}{\partial x_{1}\partial x_{2}} & ... & \frac{\partial^{2}g(\mathbf{x})}{\partial x_{1}\partial x_{n}}\\
\frac{\partial^{2}g(\mathbf{x})}{\partial x_{2}\partial x_{1}} & \frac{\partial^{2}g(\mathbf{x})}{\partial x_{2}\partial x_{2}} & ... & \frac{\partial^{2}g(\mathbf{x})}{\partial x_{2}\partial x_{n}}\\
... & ... & ... & ...\\
\frac{\partial^{2}g(\mathbf{x})}{\partial x_{n}\partial x_{1}} & \frac{\partial^{2}g(\mathbf{x})}{\partial x_{n}\partial x_{2}} & ... & \frac{\partial^{2}g(\mathbf{x})}{\partial x_{n}\partial x_{n}}
\end{array}\right)=2A$
\end_inset


\end_layout

\end_deeper
